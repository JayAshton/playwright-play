{
   "compilerOptions":{
      "target":"es6",
      "moduleResolution":"node",
      "module":"commonjs",
      "declaration":false,
      "noLib":false,
      "emitDecoratorMetadata":true,
      "experimentalDecorators":true,
      "pretty":true,
      "allowUnreachableCode":true,
      "allowUnusedLabels":true,
      "noImplicitAny":false,
      "noImplicitReturns":false,
      "noImplicitUseStrict":false,
      "outDir":"dist/",
      "listFiles":false,
      "noEmitHelpers":true
   },
   "include":[
      "**/*.ts"
   ],
   "exclude":[
      "node_modules"
   ],
   "compileOnSave":false
}